#!/bin/sh

#stopall
docker kill ca keycloak mysql nginx
docker rm ca keycloak mysql nginx
docker rmi -f cedar-ca:${CEDAR_DOCKER_VERSION} \
       cedar-keycloak:${CEDAR_DOCKER_VERSION} \
       cedar-mysql:${CEDAR_DOCKER_VERSION} \
       cedar-nginx:${CEDAR_DOCKER_VERSION}

docker volume rm cedar_ca \
       certs_and_keys_mysql certs_and_keys_nginx \
       certs_and_keys_keycloak
